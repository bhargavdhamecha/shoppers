<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.ce016.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TableGenerator</servlet-name>
        <servlet-class>com.ce016.TableGenerator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pageCount</servlet-name>
        <servlet-class>com.ce016.pageCount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calc</servlet-name>
        <servlet-class>com.ce016.Calc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newLogin</servlet-name>
        <servlet-class>com.ce016.newLogin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginservlet.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TableGenerator</servlet-name>
        <url-pattern>/tablegenerator.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pageCount</servlet-name>
        <url-pattern>/pagecount.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calc</servlet-name>
        <url-pattern>/calc.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newLogin</servlet-name>
        <url-pattern>/newlogin.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
